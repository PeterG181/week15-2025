// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Mahasiswa {
  id        Int     @id @default(autoincrement())
  nama      String
  nim       String  @unique
  jurusan   String
}